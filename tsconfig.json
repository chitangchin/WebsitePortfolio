  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/DarkModeButton.jsx", "src/pages/openAIDalle.tss", "src/pages/OpenAIDalle.jsx", "src/pages/GuestBook.jsx", "src/pages/api/GuestBook.ja", "src/pages/api/GuestBook.js", "src/pages/api/todos.jsx", "src/pages/api/GuestBook.js", "src/pages/GuestBook.jsx", "src/components/GuestBookListDisplay.jsx", "src/components/GuestBookForm.jsx"],
  "exclude": ["node_modules"]
}
